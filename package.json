{
  "name": "@exogroup/ngx-datatable",
  "version": "1.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:lib:prod": "ng build @exogroup/ngx-datatable --configuration production",
    "copy-files": "cp README.md dist/exogroup/ngx-datatable/README.md && cp LICENSE dist/exogroup/ngx-datatable/LICENSE && cpx \"src/assets/**/*\" \"dist/exogroup/ngx-datatable/assets\" && cpx \"projects/exogroup/ngx-datatable/src/lib/themes/**/*\" \"dist/exogroup/ngx-datatable/themes\"",
    "build:css": "scss-bundle  -c ./scss-bundle.config.json && node-sass -o dist/exogroup/ngx-datatable/themes dist/exogroup/ngx-datatable/themes && node-sass -o dist/exogroup/ngx-datatable dist/exogroup/ngx-datatable",
    "test": "run-p lint test:unit",
    "test:unit": "ng test @exogroup/ngx-datatable --watch=false",
    "test:watch": "ng test @exogroup/ngx-datatable",
    "test:ci": "ng test @exogroup/ngx-datatable --watch=false --progress=false --browsers=ChromeHeadlessCI",
    "ci": "run-s lint prettier:ci test:ci",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "prettier": "prettier --write \"{src,projects}/*.{js,ts,html,scss,css,md,json}\"",
    "prettier:ci": "prettier --check \"{src,projects}/*.{js,ts,html,scss,css,md,json}\"",
    "build-docs": "cross-env NODE_ENV=production ng build --configuration production --base-href=\"/ngx-datatable/\"",
    "predeploy-docs": "npm run build-docs",
    "deploy-docs": "angular-cli-ghpages --dir ./dist/ngx-datatable",
    "package": "run-s build:lib:prod copy-files build:css",
    "prepublish:lib": "run-s ci package",
    "publish:lib": "npm publish ./dist/exogroup/ngx-datatable",
    "publish:package": "npm run build:lib:prod && npm publish ./dist/exogroup/ngx-datatable && cd ../../../",
    "prepack": "npm run package",
    "pack": "npm pack ./dist/exogroup/ngx-datatable/",
    "publish": "run-s publish:lib deploy-docs",
    "prepublishOnly": "echo \"This is the workspace -- you probably meant run 'npm run publish'.\" && exit 1"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^14.3.0",
    "@angular/common": "^14.3.0",
    "@angular/compiler": "^14.3.0",
    "@angular/core": "^14.3.0",
    "@angular/forms": "^14.3.0",
    "@angular/platform-browser": "^14.3.0",
    "@angular/platform-browser-dynamic": "^14.3.0",
    "@angular/router": "^14.3.0",
    "rxjs": "^6.6.7",
    "tslib": "^2.3.1",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~14.2.13",
    "@angular-eslint/builder": "14.4.0",
    "@angular-eslint/eslint-plugin": "14.4.0",
    "@angular-eslint/eslint-plugin-template": "14.4.0",
    "@angular-eslint/schematics": "14.4.0",
    "@angular-eslint/template-parser": "14.4.0",
    "@angular/cli": "~14.2.13",
    "@angular/compiler-cli": "^14.3.0",
    "@angular/language-service": "^14.3.0",
    "@swimlane/prettier-config-swimlane": "^3.0.2",
    "@types/jasmine": "^3.6.11",
    "@types/jasminewd2": "^2.0.10",
    "@types/node": "^12.20.27",
    "@typescript-eslint/eslint-plugin": "5.43.0",
    "@typescript-eslint/parser": "5.43.0",
    "angular-cli-ghpages": "^0.6.0",
    "codelyzer": "^6.0.2",
    "cpx": "^1.5.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.28.0",
    "husky": "^4.3.8",
    "jasmine": "^3.9.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "6.4.0",
    "karma-chrome-launcher": "3.1.0",
    "karma-cli": "2.0.0",
    "karma-coverage": "^2.2.0",
    "karma-coverage-istanbul-reporter": "3.0.2",
    "karma-jasmine": "5.1.0",
    "karma-jasmine-html-reporter": "2.0.0",
    "karma-mocha-reporter": "2.2.5",
    "ng-packagr": "^14.2.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.4.1",
    "pretty-quick": "^2.0.1",
    "protractor": "~7.0.0",
    "scss-bundle": "^3.1.1",
    "ts-node": "~7.0.0",
    "tslint": "~6.1.0",
    "tslint-config-prettier": "^1.17.0",
    "tslint-config-security": "^1.16.0",
    "tslint-config-swimlane": "^5.0.0",
    "typescript": "~4.8.4"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  }
}
